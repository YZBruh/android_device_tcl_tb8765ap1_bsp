# mount point               fstype              device                 device2                   flags

# Main Partitions ext4
/system_root                 ext4      /dev/block/by-name/system            flags=display="System";backup=1;flashimg=1;ro
/vendor                      ext4      /dev/block/by-name/vendor            flags=display="Vendor";backup=1;flashimg=1;rw
/data                        ext4      /dev/block/by-name/userdata          flags=display="Data";backup=1;flashimg=1;noatime;nosuid;nodev;noauto_da_alloc;errors=panic;wait;check;formattable;quota;reservedsize=128m;formattable;resize;forcefdeorfbe=/dev/block/platform/bootdevice/by-name/metadata
/cache                       ext4      /dev/block/by-name/cache             flags=display="Cache";backup=1;flashimg=1
